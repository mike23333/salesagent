# LiveKit Configuration
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# Gemini Live API - Get your key from https://aistudio.google.com/apikey
GOOGLE_API_KEY=

# Firebase Configuration (optional - mock mode if not set)
# Download service account key from Firebase Console
GOOGLE_APPLICATION_CREDENTIALS=/path/to/firebase-service-account.json

# Telegram Webhook URL (for send_link tool)
TELEGRAM_WEBHOOK_URL=http://localhost:8000/send-link

# Telegram MTProto Configuration (for Telethon)
# Get API_ID and API_HASH from https://my.telegram.org/apps
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
# Generate session string by running: python -c "from services.telegram_webhook import *; ..."
# See src/services/telegram_webhook.py for instructions
TELEGRAM_SESSION_STRING=
